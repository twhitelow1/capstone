@chores.map do |chore|
  {
    id: chore.id,
    title: chore.title,
    desc: chore.desc,
    frequency: chore.frequency,
    last_completed: chore.last_completed,
    points_gain: chore.points_gain,
    points_price: chore.points_price,
    room_id: chore.room_id,
    currently_assigned: chore.currently_assigned,
  }
end
